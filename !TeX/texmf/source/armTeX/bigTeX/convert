|
| Converts TeX from pascal to C
| One arg must be given, giving the lib directory
echo Concatenating...
fcat %0.common_def %0.texmf_def %0.riscos_def @.tex.p >temp
echo web2c...
web2c -t -hh.texmf <temp >temp2
echo fixwrites...
fixwrites -t <temp2 >temp
echo regfix...
regfix <temp >temp2
echo splitup...
splitup <temp2
remove temp2
remove temp
