    printnl ( 262 ) ;
    print ( 286 ) ;
  }
  print ( s ) ;
  printchar ( 61 ) ;
  printint ( n ) ;
  printchar ( 93 ) ;
  {
    helpptr = 2 ;
    helpline [ 1 ] = 287 ;
    helpline [ 0 ] = 288 ;
  }
  {
    if ( interaction == 3 )
    interaction = 2 ;
    if ( logopened )
    error () ;
	;
#ifdef DEBUG
    if ( interaction > 0 )
    debughelp () ;
#endif /* DEBUG */
    history = 3 ;
    jumpout () ;
  }
}
