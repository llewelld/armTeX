    }
    tfmtemp = getc ( tfmfile ) ;
    {
      ec = tfmtemp ;
      if ( ec > 127 )
      goto lab11 ;
      tfmtemp = getc ( tfmfile ) ;
      ec = ec * 256 + tfmtemp ;
    }
    if ( ( bc > ec + 1 ) || ( ec > 255 ) )
    goto lab11 ;
    if ( bc > 255 )
    {
      bc = 1 ;
      ec = 0 ;
    }
    tfmtemp = getc ( tfmfile ) ;
    {
      nw = tfmtemp ;
      if ( nw > 127 )
      goto lab11 ;
      tfmtemp = getc ( tfmfile ) ;
