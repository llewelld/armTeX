  internalfontnumber f  ;
  internalfontnumber g  ;
  eightbits a, b, c, d  ;
  fourquarters qw  ;
  scaled sw  ;
  integer bchlabel  ;
  integer bchar  ;
  scaled z  ;
  integer alpha  ;
  char beta  ;
  g = 0 ;
  fileopened = false ;
  packfilename ( nom , aire , 805 ) ;
  if ( ! bopenin ( tfmfile ) )
  {
    if ( maketextfm () )
