    print ( 738 ) ;
    printscaled ( s ) ;
    print ( 394 ) ;
  }
  else if ( s != -1000 )
  {
    print ( 797 ) ;
    printint ( - (integer) s ) ;
  }
  if ( fileopened )
  print ( 798 ) ;
  else print ( 799 ) ;
  {
    helpptr = 5 ;
    helpline [ 4 ] = 800 ;
    helpline [ 3 ] = 801 ;
    helpline [ 2 ] = 802 ;
    helpline [ 1 ] = 803 ;
    helpline [ 0 ] = 804 ;
  }
  error () ;
