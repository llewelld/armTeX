  lab2: {

    dvibuf [ dviptr ] = w / 256 ;
    incr ( dviptr ) ;
    if ( dviptr == dvilimit )
    dviswap () ;
  }
  lab1: {

    dvibuf [ dviptr ] = w % 256 ;
    incr ( dviptr ) ;
    if ( dviptr == dvilimit )
    dviswap () ;
  }
  return ;
  lab40: mem [ q ] .hh .v.LH = mem [ p ] .hh .v.LH ;
  if ( mem [ q ] .hh .v.LH == 1 )
  {
    {
      dvibuf [ dviptr ] = o + 4 ;
