		lx = ( 2 * lr + lq + 1 ) / ( 2 * lq + 2 ) ;
		curh = curh + ( ( lr - ( lq - 1 ) * lx ) / 2 ) ;
	      }
	    }
	    while ( curh + leaderwd <= edge ) {

	      curv = baseline + mem [ leaderbox + 4 ] .cint ;
	      if ( curv != dviv )
	      {
		movement ( curv - dviv , 157 ) ;
