    }
    if ( lf != 6 + lh + ( ec - bc + 1 ) + nw + nh + nd + ni + nl + nk + ne +
    np )
    goto lab11 ;
  }
  lf = lf - 6 - lh ;
  if ( np < 7 )
  lf = lf + 7 - np ;
  if ( ( fontptr == fontmax ) || ( fmemptr + lf > fontmemsize ) )
  {
