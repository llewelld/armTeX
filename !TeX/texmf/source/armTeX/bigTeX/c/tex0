	beta ;
	if ( a == 0 )
	fontinfo [ parambase [ f ] + k - 1 ] .cint = sw ;
	else if ( a == 255 )
	fontinfo [ parambase [ f ] + k - 1 ] .cint = sw - alpha ;
	else goto lab11 ;
      }
