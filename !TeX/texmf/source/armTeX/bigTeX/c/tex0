	    ruleht = mem [ leaderbox + 3 ] .cint ;
	    ruledp = mem [ leaderbox + 2 ] .cint ;
	    goto lab14 ;
	  }
	  leaderwd = mem [ leaderbox + 1 ] .cint ;
	  if ( ( leaderwd > 0 ) && ( rulewd > 0 ) )
	  {
	    rulewd = rulewd + 10 ;
