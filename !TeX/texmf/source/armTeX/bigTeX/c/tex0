	  tfmtemp = getc ( tfmfile ) ;
	  d = tfmtemp ;
	  qw .b3 = d ;
	  fontinfo [ k ] .qqqq = qw ;
	}
	if ( a > 128 )
	{
	  if ( 256 * c + d >= nl )
	  goto lab11 ;
	  if ( a == 255 )
	  if ( k == ligkernbase [ f ] )
	  bchar = b ;
	}
	else {

	  if ( b != bchar )
	  {
	    {
