    if ( feof ( tfmfile ) )
    goto lab11 ;
    {register integer for_end; k = np + 1 ; for_end = 7 ; if ( k <= for_end)
    do
      fontinfo [ parambase [ f ] + k - 1 ] .cint = 0 ;
    while ( k++ < for_end ) ; }
  }
  if ( np >= 7 )
  fontparams [ f ] = np ;
  else fontparams [ f ] = 7 ;
  hyphenchar [ f ] = eqtb [ 12709 ] .cint ;
  skewchar [ f ] = eqtb [ 12710 ] .cint ;
