      if ( ! bopenin ( tfmfile ) )
      goto lab11 ;
    }
    else goto lab11 ;
  }
  fileopened = true ;
  {
    {
      lf = tfmtemp ;
      if ( lf > 127 )
      goto lab11 ;
      tfmtemp = getc ( tfmfile ) ;
      lf = lf * 256 + tfmtemp ;
    }
    tfmtemp = getc ( tfmfile ) ;
    {
      lh = tfmtemp ;
      if ( lh > 127 )
      goto lab11 ;
      tfmtemp = getc ( tfmfile ) ;
      lh = lh * 256 + tfmtemp ;
    }
    tfmtemp = getc ( tfmfile ) ;
