void begindiagnostic ( )
{begindiagnostic_regmem
  oldsetting = selector ;
  if ( ( eqtb [ 12692 ] .cint <= 0 ) && ( selector == 19 ) )
  {
    decr ( selector ) ;
    if ( history == 0 )
    history = 1 ;
  }
}
void zenddiagnostic ( blankline )
boolean blankline ;
{enddiagnostic_regmem
  printnl ( 335 ) ;
  if ( blankline )
  println () ;
  selector = oldsetting ;
}
