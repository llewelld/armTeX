  printchar ( 42 ) ;
  else {

    printscaled ( mem [ p + 1 ] .cint ) ;
    if ( s != 0 )
    print ( s ) ;
    if ( mem [ p + 2 ] .cint != 0 )
    {
      print ( 310 ) ;
      printglue ( mem [ p + 2 ] .cint , mem [ p ] .hh.b0 , s ) ;
    }
    if ( mem [ p + 3 ] .cint != 0 )
