      helpline [ 4 ] = 722 ;
      helpline [ 3 ] = 723 ;
      helpline [ 2 ] = 709 ;
      helpline [ 1 ] = 710 ;
      helpline [ 0 ] = 711 ;
    }
    error () ;
    goto lab32 ;
  }
  curval = xnoverd ( curval , num , denom ) ;
  f = ( num * f + 65536L * texremainder ) / denom ;
  curval = curval + ( f / 65536L ) ;
  f = f % 65536L ;
  lab32: ;
  lab88: if ( curval >= 16384 )
  aritherror = true ;
  else curval = curval * 65536L + f ;
  lab30: ;
  {
    getxtoken () ;
    if ( curcmd != 10 )
