  fontarea [ f ] = aire ;
  fontbc [ f ] = bc ;
  fontec [ f ] = ec ;
  fontglue [ f ] = 0 ;
  charbase [ f ] = charbase [ f ] ;
  widthbase [ f ] = widthbase [ f ] ;
  ligkernbase [ f ] = ligkernbase [ f ] ;
  kernbase [ f ] = kernbase [ f ] ;
  extenbase [ f ] = extenbase [ f ] ;
