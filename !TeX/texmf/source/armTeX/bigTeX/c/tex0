      printesc ( 497 ) ;
      printint ( n - 13251 ) ;
    }
    printchar ( 61 ) ;
    printscaled ( eqtb [ n ] .cint ) ;
    print ( 394 ) ;
  }
  else printchar ( 63 ) ;
}
