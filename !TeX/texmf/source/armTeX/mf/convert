echo Concatenating...
fcat %0.common_def %0.texmf_def %0.riscos_def @.mf.p >temp
echo First sed run...
sed -f "<Obey$Dir>.SedScr1" temp >temp2
echo web2c...
web2c -hh.texmf -m <temp2 >temp
echo second sed run...
sed -f "<Obey$Dir>.SedScr2" temp >temp2
echo fixwrites...
fixwrites <temp2 >temp
echo splitup...
splitup mf <temp
fcat h.coerce h.fixcoerce >h.xcoerce
copy h.xcoerce h.coerce ~cd~v
remove temp
remove temp2
